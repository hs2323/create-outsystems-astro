{
  "name": "create-outsystems-astro",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "astro dev",
    "format": "prettier . --check",
    "format:write": "prettier  --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "output": "astro build && ts-node ./output.ts",
    "output:bun": "astro build && bun run ./output.ts",
    "output:deno": "astro build && deno run -A ./output.ts",
    "postinstall": "patch-package",
    "preview": "astro preview",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:deno": "PW_DISABLE_TS_ESM=true deno run --allow-all npm:playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ui:deno": "PW_DISABLE_TS_ESM=true deno run --allow-all npm:playwright test --ui",
    "test:e2e:install": "playwright install --with-deps",
    "test:run": "vitest run",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@analogjs/astro-angular": "^2.2.1",
    "@angular/animations": "^21.0.8",
    "@angular/build": "^21.0.4",
    "@angular/common": "^21.0.8",
    "@angular/compiler": "^21.0.8",
    "@angular/compiler-cli": "^21.0.8",
    "@angular/core": "^21.0.8",
    "@angular/language-service": "^21.0.8",
    "@angular/platform-browser": "^21.0.8",
    "@angular/platform-server": "^21.0.8",
    "@astrojs/react": "^4.4.2",
    "@astrojs/vue": "^5.1.3",
    "astro": "^5.16.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rxjs": "^7.8.2",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.1",
    "@analogjs/vitest-angular": "^2.2.1",
    "@angular/router": "^21.0.8",
    "@eslint/compat": "^2.0.1",
    "@eslint/js": "^9.39.2",
    "@eslint/markdown": "^7.5.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/angular": "^19.0.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.3",
    "angular-eslint": "^21.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-perfectionist": "^5.3.1",
    "eslint-plugin-playwright": "^2.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-testing-library": "^7.15.4",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "happy-dom": "^20.1.0",
    "js-beautify": "^1.15.4",
    "patch-package": "^8.0.1",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "^10.2.0"
  },
  "overrides": {
    "hono": "4.11.4"
  },
  "pnpm": {
    "overrides": {
      "hono": "4.11.4"
    }
  }
}
